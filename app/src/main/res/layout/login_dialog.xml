<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/juli_15dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginTop="25dp"
            android:background="@color/color_find_gray" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/dialog_cs"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/common_padding_super_large"
                    android:text="城市"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/dialog_view1"
                    android:layout_width="match_parent"
                    android:layout_height="3dp"
                    android:layout_marginLeft="@dimen/common_padding_super_large"
                    android:layout_marginTop="2dp"
                    android:background="@color/them_color" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/dialog_qx"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/min_height_min"
                    android:text="区县"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/dialog_view2"
                    android:layout_width="wrap_content"
                    android:layout_height="3dp"
                    android:layout_marginLeft="@dimen/min_height_min"
                    android:layout_marginTop="2dp"
                    android:background="@color/them_color"
                    android:visibility="gone" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/dialog_sk"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/min_height_min"
                    android:text="水库"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/dialog_view3"
                    android:layout_width="wrap_content"
                    android:layout_height="3dp"
                    android:layout_marginLeft="@dimen/min_height_min"
                    android:layout_marginTop="2dp"
                    android:background="@color/them_color"
                    android:visibility="gone" />
            </LinearLayout>
        </LinearLayout>

        <TextView
            android:id="@+id/dialog_qd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/common_margin_super_large"
            android:text="确定"
            android:textSize="16sp" />
    </RelativeLayout>


